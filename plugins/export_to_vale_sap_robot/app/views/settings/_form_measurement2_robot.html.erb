<table>
  <tbody>
  <tr>
    <th><%= t('label_tracker') %>:</th>
    <td>
      <%= select_tag 'settings[measurement2_tracker_id]', options_from_collection_for_select(Tracker.all, :id, :name, settings['measurement2_tracker_id']), include_blank: true %>
    </td>
  </tr>
  <tr>
    <th><%= t('export_to_vale_sap_robot.wating_status') %>:</th>
