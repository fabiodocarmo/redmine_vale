<% @issue_custom_fields_grid = IssueCustomField.where(field_format: 'grid') %>
<% @issue_custom_fields_float = IssueCustomField.where(field_format: 'float') %>
<% @issue_custom_fields_string = IssueCustomField.where(field_format: 'string') %>
