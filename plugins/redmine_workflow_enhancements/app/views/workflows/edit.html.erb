<%= render :super %>

<% if @tracker && @role || @trackers && @roles %>
  <% content_for :header_tags do -%>
    <%= stylesheet_link_tag "workflow_enhancements.css", :plugin => :redmine_workflow_enhancements %>
  <% end -%>

  <div class="workflowGraphContainer">
    <% if @tracker && @role %>
      <%= render :partial => 'workflow_enhancements/workflow_graph', :locals => {
          :trackers => @tracker, :roles => @role} %>
    <% else %>
      <%= render :partial => 'workflow_enhancements/workflow_graph', :locals => {
          :trackers => @trackers, :roles => @roles} %>
    <% end %>
  </div>
<% end %>
